%section.page-header
  .row
    .col-md-8
      %h1 プロジェクト
    .col-md-4
      = link_to 'Launch new project', new_project_url, class: 'btn btn-secondary pull-right'
%section.content
  .row
    .col-md-12
      %table.table.actions
        %thead
          %tr
            %th Name
            %th Goal
            %th
        %tbody
          = cell(Project::Cell::Item, collection: model)
